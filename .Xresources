!#include "$HOME/.cache/wal/colors.Xresources"
! X colors.
! Generated by 'wal'
*foreground:        #8bebc5
*background:        #0b1118
*.foreground:       #8bebc5
*.background:       #0b1118
emacs*foreground:   #8bebc5
emacs*background:   #0b1118
URxvt*foreground:   #8bebc5
XTerm*foreground:   #8bebc5
UXTerm*foreground:  #8bebc5
URxvt*background:   [100]#0b1118
XTerm*background:   #0b1118
UXTerm*background:  #0b1118
URxvt*cursorColor:  #8bebc5
XTerm*cursorColor:  #8bebc5
UXTerm*cursorColor: #8bebc5
URxvt*borderColor:  [100]#0b1118

! Colors 0-15.
*.color0: #0b1118
*color0:  #0b1118
*.color1: #554C41
*color1:  #554C41
*.color2: #962A30
*color2:  #962A30
*.color3: #F21B2B
*color3:  #F21B2B
*.color4: #DF4D49
*color4:  #DF4D49
*.color5: #2EA564
*color5:  #2EA564
*.color6: #F7934A
*color6:  #F7934A
*.color7: #8bebc5
*color7:  #8bebc5
*.color8: #61a489
*color8:  #61a489
*.color9: #554C41
*color9:  #554C41
*.color10: #962A30
*color10:  #962A30
*.color11: #F21B2B
*color11:  #F21B2B
*.color12: #DF4D49
*color12:  #DF4D49
*.color13: #2EA564
*color13:  #2EA564
*.color14: #F7934A
*color14:  #F7934A
*.color15: #8bebc5
*color15:  #8bebc5

! Black color that will not be affected by bold highlighting.
*.color66: #0b1118
*color66:  #0b1118

! Xclock colors.
XClock*foreground: #8bebc5
XClock*background: #0b1118
XClock*majorColor:  rgba:8b/eb/c5/ff
XClock*minorColor:  rgba:8b/eb/c5/ff
XClock*hourColor:   rgba:8b/eb/c5/ff
XClock*minuteColor: rgba:8b/eb/c5/ff
XClock*secondColor: rgba:8b/eb/c5/ff

! Set depth to make transparency work.
URxvt*depth: 32

!!URxvt appearance
URxvt.font: xft:Iosevka Term:style=Regular:size=12, xft:Source Han Sans JP:style=Regular:size=14
URxvt.lineSpace: 0
URxvt.letterSpace: 0
URxvt.internalBorder: 25
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_left: false
URxvt.saveline: 2048
URxvt.iso14755: false
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
